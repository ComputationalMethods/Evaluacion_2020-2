        "id": "F8NQIK9GZ8ps"
      },
      "source": [
        "def argeig(A):\n",
        "  '''\n",
        "  Funci√≥n que diagonaliza con orden ascendente en los autovalores\n",
        "  '''\n",
        "  \n",
        "  l,V = np.linalg.eig(A)\n",
        "  index = np.abs(l).argsort()\n",
        "  argl = np.sort(l)\n",
        "  argV = np.c_[tuple([np.c_[V[:,i]] for i in index])]\n",
        "\n",
        "  return argl, argV"
      ],
      "execution_count": 2,
      "outputs": []
    }
